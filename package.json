{
  "name": "webpack-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "npm run cleanScripts && npm run modernizr:app && npm run scripts:app",
    "cleanScripts": "rm -rf ./app/_scripts",
    "modernizr:app": "./node_modules/.bin/customizr -c ./tasks/modernizr.json",
    "scripts:app": "webpack -w",
    "start": "npm run clean:dist && npm run copy:dist && npm run scripts:dist && npm run versionAssets && npm run cleanComponents",
    "clean:dist": "rm -rf ./dist/",
    "copy:dist": "cp -r ./app/ ./dist/ && rm -rf ./dist/_scripts",
    "scripts:dist": "NODE_ENV=production webpack -p",
    "cleanComponents": "rm -rf ./dist/components",
    "versionAssets": "node ./tasks/versionassets.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "customizr": "^1.0.0-alpha",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs": "0.0.2",
    "fs-extra": "^0.29.0",
    "globby": "^4.0.0",
    "imagemin": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-version-assets": "^1.1.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "svg-sprite": "^1.2.19",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "delegate-events": "^1.1.1"
  }
}
