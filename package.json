{
  "name": "webpack-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean:scripts:dev && npm run modernizr:dev && npm run scripts:dev",
    "clean:scripts:dev": "rm -rf ./app/_scripts",
    "modernizr:dev": "./node_modules/.bin/customizr -c ./tasks/modernizr.json",
    "scripts:dev": "webpack -w",
    "build": "npm run clean:dist && npm run copy:dist && npm run modernizr:dist && npm run scripts:dist && npm run clean:dist:scripts & npm run images:dist && npm run rev:dist",
    "clean:dist": "rm -rf ./dist/",
    "copy:dist": "cp -r ./app/ ./dist/ && rm -rf ./dist/_scripts && rm -rf ./dist/images",
    "modernizr:dist": "npm run modernizr:dev && mkdir ./dist/_scripts && cp ./app/_scripts/modernizr.js ./dist/_scripts/",
    "scripts:dist": "NODE_ENV=production webpack -p --config webpack.production.config.babel.js",
    "clean:dist:scripts": "rm -rf ./dist/components",
    "images:dist": "node ./tasks/imageMin.js",
    "rev:dist": "node ./tasks/revAssets.js",
    "new": "node ./tasks/newModule.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "customizr": "^1.0.0-alpha",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.29.0",
    "globby": "^4.0.0",
    "imagemin": "^4.0.0",
    "node-version-assets": "^1.1.0",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "delegate-events": "^1.1.1"
  }
}
