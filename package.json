{
  "name": "webpack-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean:scripts && webpack -w",
    "build": "npm run clean:dist && npm run copy:dist && npm run scripts:dist && npm run clean:dist:scripts && npm run rev:dist",
    "clean:scripts": "trash ./app/_scripts/*",
    "new": "node ./tasks/newModule.js",
    "clean:dist": "trash ./dist/",
    "copy:dist": "cp -r ./app/ ./dist/ && trash ./dist/_scripts",
    "scripts:dist": "NODE_ENV=production webpack -p --config webpack.production.config.babel.js",
    "clean:dist:scripts": "trash ./dist/components",
    "rev:dist": "node ./tasks/revAssets.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "globby": "^4.0.0",
    "node-version-assets": "^1.1.0",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "delegate-events": "^1.1.1"
  }
}
